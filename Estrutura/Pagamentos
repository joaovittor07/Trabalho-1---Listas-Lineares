package Estrutura;

public class Pagamentos {
    private double valor;
    private String metodo;

    public Pagamentos(double valor, String metodo) {
        this.valor = valor;
        this.metodo = metodo;
    }

    public double getValor() {
        return valor;
    }

    public String getMetodo() {
        return metodo;
    }

    @Override
    public String toString() {
        return "Pagamento{" +
                "valor=" + valor +
                ", metodo='" + metodo + '\'' +
                '}';
    }
}
